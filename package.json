{
  "name": "simple-blog-frontend-jade",
  "version": "1.0.0",
  "description": "Simple blog frontend with Jade",
  "main": "server.js",
  "scripts": {
    "babel": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./public --copy-files",
    "babel:watch": "rimraf babel-watch ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "nodemon dist/server.js",
    "dev": "npm-run-all -s sass:build babel start -p babel:watch sass:watch",
    "sass:build": "node-sass public/sass -o dist/public/css",
    "sass:watch": "node-sass -w public/sass -o dist/public/css",
    "test": "mocha",
    "gulp": "gulp"
  },
  "keywords": [
    "simple",
    "blog",
    "frontend",
    "jade",
    "nodejs"
  ],
  "author": "Daniel Jimenez <jimenezdaniel87@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "helmet": "^3.8.1",
    "jade": "^1.11.0",
    "moment": "^2.18.1",
    "nodemon": "^1.11.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "mocha": "^3.5.0"
  }
}
